{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "pnpm install && pnpm build"
    },
    "installCommand": "pnpm install",
    "startCommand": "pnpm start"
  }